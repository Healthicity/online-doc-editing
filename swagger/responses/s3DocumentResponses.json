        "schema": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "ETag": {
                "type": "string",
                "description": "An entity tag of the document to be able to compare to other objects or files"
              },
              "ChecksumAlgorithm": {
                "type": "array",
                "items": {}
              },
              "Size": {
                "type": "integer",
                "description": "The size of the file"
              },
              "StorageClass": {
                "type": "string",
                "description": "The type of storage class of the file"
              },
              "Key": {
                "type": "string",
                "description": "The name of the file"
              },
              "VersionId": {
                "type": "string",
                "description": "The unique Id of the version document",
                "nullable": false
              },
              "IsLatest": {
                "type": "boolean",
                "description": "Represents if the version is the latest version, thus, is the main document.",
                "nullable": false
              },
              "LastModified": {
                "type": "string",
                "format": "date-time",
                "description": "The last date it was updated"
              },
              "Owner": {
                "type": "object",
                "properties": {
                  "DisplayName": {
                    "type": "string",
                    "description": "The display name"
                  },
                  "ID": {
                    "type": "string",
                    "description": "The ID of the owner"
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "RestoreObject200Response": {
    "description": "One document with a delete marker successfully restored!",
    "content": {
      "application/json": {
        "schema": {
          "type": "object",
          "properties": {
            "message": {
              "type": "string",
              "default": "Document: ${key} successfully restored!"
            }
          }
        }
      }
    }
  }
}
